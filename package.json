{
  "type": "module",
  "name": "dms",
  "version": "1.0.0",
  "description": "a document management system",
  "main": "serverProd.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "start": "node serverProd.js",
    "seed": "sequelize db:seed --seed roles.js && sequelize db:seed --seed users.js && sequelize db:seed --seed documents.js",
    "start:dev": "nodemon serverDev.js",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html --extension .jsx ./node_modules/mocha/bin/_mocha client/tests/testSetup.js client/tests/components/**/*spec.js client/tests/**/*spec.js server/tests/**/*.spec.js --timeout=60000",
    "test:server": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/mocha/bin/_mocha server/tests/**/*.spec.js --timeout=60000",
    "test:client": "NODE_ENV=test nyc --reporter=text --reporter=html --extension .jsx ./node_modules/mocha/bin/_mocha client/tests/testSetup.js client/tests/components/**/*spec.js client/tests/**/*spec.js --timeout=60000",
    "clean-dist": "npm run remove-dist && mkdir -p client/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "remove-dist": "rimraf client/dist",
    "test:models": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/mocha/bin/_mocha server/tests/models/*.spec.js --timeout=60000",
    "build:html": "node tools/buildHtml.js",
    "prebuild": "npm-run-all build:html",
    "build": "node tools/build.js",
    "postbuild": "node serverProd.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "server/tests",
      "client/tests"
    ]
  },
  "author": "Oluwagbenga Joloko",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "npmlog": "^4.1.0",
    "pg": "^6.2.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.4",
    "swagger-jsdoc": "^1.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.7.0",
    "nyc": "^11.0.2",
    "mocha": "^3.4.2",
    "chai": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andela-ojoloko/CP-2-Document-Mangement-system"
  }
}
